terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 3.0"
    }
  }
}
provider "aws" {
  region = "us-east-1"
  profile = "default"
}


module "windowsserver" {

  source = "./modules/ec2"
     ami = data.aws_ami.windows.id
     instance_type = "t2.micro"
     keyname = "windows"
     subnet = "subnet-07a389cc2ad2d24f7"
     securitygroup = [ "sg-0a4fe4ebbcbc0d8ff", "sg-02cc3a69f72a00982"]
     

}








































































































































































































































































































































































































